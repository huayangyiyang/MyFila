   <script>
       
           

           $('#input_login_name').blur(function () {
               if ($('#input_login_name').val().length < 1) {
                   $('.form-spna-01').html('用户名不能为空').css('color', 'red');
                   $('#input_login_name').focus();
               } else if ($('#input_login_name').val().length < 4) {
                   $('.form-spna-01').html('用户名长度不能小于4位').css('color', 'red');
                   $('#input_login_name').focus();
               } else if ($('#input_login_name').val().length > 12) {
                   $('.form-spna-01').html('用户名长度不能大于12位').css('color', 'red');
                   $('#input_login_name').focus();
               } else if (!(/^[a-zA-Z0-9_-]{4,12}$/.test($('#input_login_name').val()))) {
                   $('.form-spna-01').html('用户名格式不正确').css('color', 'red');
                   $('#input_login_name').focus();
               }
               else {
                   $('.form-spna-01').html('正确').hide();
               }
           });

           $('#input_login_name').keyup(function () {
               if ($('#input_login_name').val().length < 1) {
                   $('#input_login_name').focus();
                   $('.form-spna-01').html('用户名不能为空').css('color', 'red');
               } else if ($('#input_login_name').val().length < 4) {
                   $('.form-spna-01').html('用户名长度不能小于4位').css('color', 'red');
                   $('#input_login_name').focus();
               } else if ($('#input_login_name').val().length > 12) {
                   $('.form-spna-01').html('用户名长度不能大于12位').css('color', 'red');
                   $('#input_login_name').focus();
               } else if (!(/^[a-zA-Z0-9_-]{4,12}$/.test($('#input_login_name').val()))) {
                   $('.form-spna-01').html('用户名格式不正确').css('color', 'red');
                   $('#input_login_name').focus();
               } else {
                   $('.form-spna-01').html('正确').hide();
               }
           });
           $('#input_login_password').focus(function () {
               if ($('#input_login_name').val().length < 1) {
                   $('#input_login_name').focus();
                   $('.form-spna-01').html('用户名不能为空').css('color', 'red');
               } else if ($('#input_login_name').val().length < 4) {
                   $('.form-spna-01').html('用户名长度不能小于4位').css('color', 'red');
                   $('#input_login_name').focus();
               } else if ($('#input_login_name').val().length > 12) {
                   $('.form-spna-01').html('用户名长度不能大于12位').css('color', 'red');
                   $('#input_login_name').focus();
               } else if (!(/^[a-zA-Z0-9_-]{4,12}$/.test($('#input_login_name').val()))) {
                   $('.form-spna-01').html('用户名格式不正确').css('color', 'red');
                   $('#input_login_name').focus();
               }
               else {
                   $('.form-spna-01').html('正确').hide();
               }
           });

           $('#input_login_password').keyup(function () {
               if ($('#input_login_password').val().length<1) {
                   $('.form-spna-02').html('密码不能为空').css('color', 'red');
               }else if ($('#input_login_password').val().length<4) {
                   $('.form-spna-02').html('密码长度不能小于4位').css('color', 'red');
               }else if ($('#input_login_password').val().length>12) {
                   $('.form-spna-02').html('密码长度不能大于16位').css('color', 'red');
               } else if (!(/^[a-zA-Z0-9_-]{4,12}$/.test($('#input_login_password').val()))) {
                   $('.form-spna-02').html('密码格式不正确').css('color', 'red');
               }

               else {
                   $('.form-spna-02').html('正确').hide();
                   
               }
           });
           $('#input_login_password').blur(function () {
               if ($('#input_login_password').val().length < 1) {
                   $('.form-spna-02').html('密码不能为空').css('color', 'red');
               } else if ($('#input_login_password').val().length < 4) {
                   $('.form-spna-02').html('密码长度不能小于4位').css('color', 'red');
               } else if ($('#input_login_password').val().length > 12) {
                   $('.form-spna-02').html('密码长度不能大于12位').css('color', 'red');
               } else if(!(/^[a-zA-Z0-9_-]{4,12}$/.test($('#input_login_password').val()))) {
                   $('.form-spna-02').html('密码格式不争取').css('color', 'red');
               }

               else {
                   $('.form-spna-02').html('正确').hide();
                   
               }
           });
           
