    <div class="container main-container main">
        <div class="passport-signup-container">
            <div class="panel panel-default signup-panel">
                <div class="panel-heading clearfix">
                    <span >欢迎登录</span>
                    <a href="/"><img src="~/Images/login-logo.png" alt=""></a>
                    <!-- <a href="/"><img src="static/picture/login-logo.png" alt=""></a> -->
                </div>
                <div class="panel-body">
                    <form id="member_signup_form" class="form-horizontal" action="~/User/Login" method="post">
                        <!-- 登陆后跳转地址 -->
                        <div class="form-group">
                            <span class="form-spna-01">@ViewBag.wrongMessage</span>
                        </div>
                        <div class="form-group">
                            <div class="col-md-5">
                                <input type="text"  class="form-control" name="login_username" id="input_login_name" placeholder="用户名4-12为大小字母数字下划线横线">
                            </div>
                        </div>
                        <div class="form-group">
                            <span class="form-spna-02"></span>
                        </div>
                        <div class="form-group">
                            <div class="col-md-5">
                                <input type="password" name="login_password" class="form-control" id="input_login_password" placeholder="密码4-12为大小字母数字下划线横线">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-5">
                                <button type="submit" id="btnsubmit" class="btn btn-warning">立即登录</button>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-5">
                                <a class="text-center" href="~/User/Register">没有账号？立即注册</a>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
       
    </div>

